<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.exit.portaleducando">

    <!-- Include required permissions for Google Mobile Ads to run. -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_portaleducando"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginPortalEducando" />
        <activity android:name=".Menu">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Nutricional"
            android:parentActivityName=".Menu" />
        <activity
            android:name=".Horario"
            android:parentActivityName=".Menu" />
        <activity
            android:name=".AcompMedico"
            android:parentActivityName=".Menu" />
        <activity
            android:name=".AcompEscolar"
            android:parentActivityName=".Menu" />
        <activity
            android:name=".Diario"
            android:parentActivityName=".Menu" />
        <activity
            android:name=".Localizacao"
            android:parentActivityName=".Menu" />
        <activity
            android:name=".Sobre"
            android:parentActivityName=".Menu" />
        <!-- This meta-data tag is required to use Google Play Services. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <!-- Include the AdActivity configChanges and theme. -->
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />
        <activity android:name=".Perfil"
            android:parentActivityName=".Menu"></activity>
    </application>

</manifest>