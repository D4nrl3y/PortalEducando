<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_acomp_escolar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.exit.portaleducando.AcompEscolar"
    android:orientation="vertical"
    android:weightSum="1">

    <ImageButton
        android:layout_width="107dp"
        android:src="@drawable/iconbarra"
        android:id="@+id/selecbarra"
        android:onClick="acessoDireto"
        android:layout_gravity="top|left"
        android:layout_height="35dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="107dp"
        android:layout_height="35dp"
        android:src="@drawable/iconpizza"
        android:id="@+id/selecpizza"
        android:onClick="acessoDireto2"
        android:layout_gravity="top|right"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <WebView
        android:id="@+id/webviewGraficos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/selecbarra"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <!--<RelativeLayout-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="match_parent"-->
            <!--android:gravity="center"-->
            <!--android:id="@+id/fragmentContainer"-->
